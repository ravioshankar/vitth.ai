{
  "name": "attune-reviewer",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve attune-reviewer --configuration=development --port 4300",
    "build": "ng run attune-reviewer:build:production",
    "watch": "ng build --watch --configuration development",
    "test": "ng run attune-reviewer:test",
    "postinstall": "node ./decorate-angular-cli.js",
    "prepare": "husky install",
    "doc:local": "compodoc -p ./tsconfig.compodoc.json -s -o -w",
